<UserControl x:Class="FoodSafetyMonitoring.Manager.SysHelp"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:usercontrols="clr-namespace:FoodSafetyMonitoring.Manager.UserControls">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/TextBlock.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <Grid HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,30,0,0" Width="1100" Height="500">
            <Grid.Background>
                <ImageBrush x:Name="_help" ImageSource="../res/zrd.jpg"/>
            </Grid.Background>
        </Grid>
    </Grid>
</UserControl>
